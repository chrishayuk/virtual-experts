    "time_unit": {"path": "phrases.time_unit_pairs"},
    "distance_unit": {"path": "phrases.distance_unit_pairs"}
  },
  "template_vars": {
    "a_vehicle": "vehicle.name|with_article",
    "time_unit": "time_unit.singular",
    "time_unit_plural": "time_unit.plural",
    "distance_unit": "distance_unit.plural"
  },
  "trace": [
    {"op": "init", "var": "speed", "value": "speed"},
    {"op": "init", "var": "time", "value": "time"},
