{
  "name": "entity_consume_sequence",
  "expert": "entity_track",
  "description": "Entity consumes items multiple times",
  "pattern": "entity_track",
  "variant": "consume_sequence",
  "vocab": {
    "person": {"type": "person_with_pronouns"},
    "item": {"path": "items.countable_singular"},
    "container_type": {"path": "containers.types"}
  },
  "template_vars": {
    "name": "person.name",
    "subject": "person.subject",
    "subj": "person.subject|capitalize",
    "his_her": "person.possessive",
    "items": "item|pluralize",
    "initial": "start",
    "container": "container_type",
    "eat_verb1": "uses",
    "eat_verb2": "uses"
  },
  "variables": {
    "start": {
      "type": "int",
      "min": 15,
      "max": 40
    },
    "consume1": {
      "type": "int",
      "min": 2,
      "max": 8
    },
    "consume2": {
      "type": "int",
      "min": 2,
      "max": 8
    }
  },
  "constraints": {
    "start - consume1 - consume2": {
      "min": 3
    }
  },
  "trace": [
    {
      "op": "init",
      "var": "items",
      "value": "start"
    },
    {
      "op": "consume",
      "entity": "items",
      "amount": "consume1"
    },
    {
      "op": "consume",
      "entity": "items",
      "amount": "consume2"
    },
    {
      "op": "query",
      "var": "items"
    }
  ],
  "answer": "start - consume1 - consume2"
}
