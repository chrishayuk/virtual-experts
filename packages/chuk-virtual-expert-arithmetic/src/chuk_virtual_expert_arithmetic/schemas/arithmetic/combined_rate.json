{
  "name": "combined_rate",
  "description": "Two machines combined output over time",
  "pattern": "combined_rate",
  "variables": {
    "rate1": {"type": "int", "min": 5, "max": 20},
    "rate2": {"type": "int", "min": 5, "max": 20},
    "time": {"type": "int", "min": 2, "max": 6}
  },
  "vocab": {
    "person": {"type": "person_with_pronouns"},
    "machines": {"path": "phrases.machine_names"},
    "rate_unit": {"path": "phrases.rate_units"}
  },
  "template_vars": {
    "name": "person.name",
    "his_her": "person.possessive",
    "machine1": "machines.first",
    "machine2": "machines.second"
  },
  "derived": {
    "item": "rate_unit.split('/')[0]"
  },
  "trace": [
    {"op": "init", "var": "rate_a", "value": "rate1"},
    {"op": "init", "var": "rate_b", "value": "rate2"},
    {"op": "init", "var": "time", "value": "time"},
    {"op": "compute", "compute_op": "add", "args": ["rate_a", "rate_b"], "var": "combined_rate"},
    {"op": "compute", "compute_op": "mul", "args": ["combined_rate", "time"], "var": "total"},
    {"op": "query", "var": "total"}
  ],
  "answer": "(rate1 + rate2) * time"
}
