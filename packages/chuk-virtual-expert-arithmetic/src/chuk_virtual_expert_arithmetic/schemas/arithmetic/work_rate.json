{
  "name": "work_rate",
  "description": "Rate \u00d7 time \u00f7 workers distribution",
  "variables": {
    "workers": {
      "type": "int",
      "min": 2,
      "max": 5
    },
    "time": {
      "type": "int",
      "min": 2,
      "max": 6
    },
    "rate_multiplier": {
      "type": "int",
      "min": 3,
      "max": 8
    }
  },
  "derived": {
    "rate": "workers * rate_multiplier"
  },
  "trace": [
    {
      "op": "init",
      "var": "rate",
      "value": "rate"
    },
    {
      "op": "init",
      "var": "time",
      "value": "time"
    },
    {
      "op": "init",
      "var": "workers",
      "value": "workers"
    },
    {
      "op": "compute",
      "compute_op": "mul",
      "args": [
        "rate",
        "time"
      ],
      "var": "step1"
    },
    {
      "op": "compute",
      "compute_op": "div",
      "args": [
        "step1",
        "workers"
      ],
      "var": "result"
    },
    {
      "op": "query",
      "var": "result"
    }
  ],
  "answer": "(rate * time) / workers",
  "pattern": "work_rate",
  "vocab": {
    "agent": {
      "path": "phrases.worker_types"
    },
    "item": {
      "path": "items.manufactured"
    },
    "target": {
      "path": "phrases.distribution_targets"
    },
    "time_unit_pair": {
      "path": "phrases.time_unit_pairs"
    }
  },
  "template_vars": {
    "agent1": "agent",
    "items": "item",
    "time_unit": "time_unit_pair.singular",
    "time_units": "time_unit_pair.plural",
    "targets": "target.plural",
    "target_singular": "target.singular"
  }
}
