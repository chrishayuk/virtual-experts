{
  "name": "weekly_sprints_same",
  "description": "N sprints N times per week (same number), M meters each - forces positional disambiguation",
  "variables": {
    "count": {
      "type": "int",
      "min": 2,
      "max": 5
    },
    "meters": {
      "type": "int",
      "min": 40,
      "max": 100,
      "multiple_of": 10
    }
  },
  "derived": {
    "total_sprints": "count * count",
    "total_meters": "total_sprints * meters"
  },
  "trace": [
    {
      "op": "init",
      "var": "sprints_per_session",
      "value": "count"
    },
    {
      "op": "init",
      "var": "sessions_per_week",
      "value": "count"
    },
    {
      "op": "compute",
      "compute_op": "mul",
      "args": [
        "sprints_per_session",
        "sessions_per_week"
      ],
      "var": "step1"
    },
    {
      "op": "init",
      "var": "meters_per_sprint",
      "value": "meters"
    },
    {
      "op": "compute",
      "compute_op": "mul",
      "args": [
        "step1",
        "meters_per_sprint"
      ],
      "var": "result"
    },
    {
      "op": "query",
      "var": "result"
    }
  ],
  "answer": "total_meters",
  "pattern": "activity_patterns",
  "variant": "repeated_value",
  "vocab": {
    "person": {
      "type": "person_with_pronouns"
    }
  },
  "template_vars": {
    "name": "person.name",
    "subject": "person.subject"
  }
}
