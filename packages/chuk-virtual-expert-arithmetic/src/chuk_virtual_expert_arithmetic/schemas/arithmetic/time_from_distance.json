{
  "name": "time_from_distance",
  "description": "Calculate time from distance and speed: time = distance / speed",
  "variables": {
    "distance": {
      "type": "int",
      "min": 10,
      "max": 100
    },
    "speed": {
      "type": "int",
      "min": 2,
      "max": 20
    }
  },
  "trace": [
    {
      "op": "init",
      "var": "distance",
      "value": "distance"
    },
    {
      "op": "init",
      "var": "speed",
      "value": "speed"
    },
    {
      "op": "compute",
      "compute_op": "div",
      "args": [
        "distance",
        "speed"
      ],
      "var": "result"
    },
    {
      "op": "query",
      "var": "result"
    }
  ],
  "answer": "distance / speed",
  "pattern": "time_from_distance",
  "vocab": {
    "person": {
      "type": "person_with_pronouns"
    },
    "distance_unit": {
      "path": "phrases.distance_unit_pairs"
    }
  },
  "template_vars": {
    "name": "person.name",
    "subject": "person.subject",
    "distance_unit": "distance_unit.singular"
  }
}
