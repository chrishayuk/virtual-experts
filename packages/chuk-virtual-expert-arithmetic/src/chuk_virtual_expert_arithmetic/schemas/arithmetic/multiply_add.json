{
  "name": "multiply_add",
  "description": "Count x price + extra fee",
  "pattern": "multiply_add",
  "mixins": ["person_vocab", "item_vocab"],
  "template_vars": {
    "item_singular": "item",
    "fee": "delivery"
  },
  "variables": {
    "count": {
      "type": "int",
      "min": 3,
      "max": 10
    },
    "price": {
      "type": "int",
      "min": 5,
      "max": 20
    },
    "extra": {
      "type": "int",
      "min": 5,
      "max": 20
    }
  },
  "trace": [
    {
      "op": "init",
      "var": "count",
      "value": "count"
    },
    {
      "op": "init",
      "var": "price",
      "value": "price"
    },
    {
      "op": "init",
      "var": "extra",
      "value": "extra"
    },
    {
      "op": "compute",
      "compute_op": "mul",
      "args": [
        "count",
        "price"
      ],
      "var": "step1"
    },
    {
      "op": "compute",
      "compute_op": "add",
      "args": [
        "step1",
        "extra"
      ],
      "var": "result"
    },
    {
      "op": "query",
      "var": "result"
    }
  ],
  "answer": "count * price + extra",
  "pattern": "multiply_add"
}
