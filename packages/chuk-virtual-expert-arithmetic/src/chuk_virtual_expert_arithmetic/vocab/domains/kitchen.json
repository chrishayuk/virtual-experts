{
  "name": "kitchen",
  "description": "Cooking and baking",
  "agent_types": ["appliance", "person"],
  "agent_templates": {
    "appliance": {"pattern": "Oven ${number}", "numbers": [1, 2]},
    "mixer": {"pattern": "Mixer ${letter}", "letters": ["A", "B"]},
    "person": {"source": "names.people", "pattern": "${name}"}
  },
  "items": [
    {"singular": "cookie", "plural": "cookies"},
    {"singular": "loaf", "plural": "loaves"},
    {"singular": "cake", "plural": "cakes"},
    {"singular": "muffin", "plural": "muffins"},
    {"singular": "pie", "plural": "pies"},
    {"singular": "batch", "plural": "batches"}
  ],
  "verbs": {"singular": "bakes", "plural": "bake"},
  "time_units": [
    {"singular": "hour", "plural": "hours"},
    {"singular": "minute", "plural": "minutes"}
  ]
}
