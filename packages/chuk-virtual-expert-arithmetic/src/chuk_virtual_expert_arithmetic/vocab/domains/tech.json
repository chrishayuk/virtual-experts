{
  "name": "tech",
  "description": "Computing and technology",
  "agent_types": ["server", "person"],
  "agent_templates": {
    "server": {"pattern": "Server ${letter}", "letters": ["A", "B"]},
    "node": {"pattern": "Node ${number}", "numbers": [1, 2]},
    "cpu": {"pattern": "CPU ${number}", "numbers": [1, 2]},
    "person": {"source": "names.people", "pattern": "${name}"}
  },
  "items": [
    {"singular": "request", "plural": "requests"},
    {"singular": "query", "plural": "queries"},
    {"singular": "task", "plural": "tasks"},
    {"singular": "job", "plural": "jobs"},
    {"singular": "packet", "plural": "packets"}
  ],
  "verbs": {"singular": "processes", "plural": "process"},
  "time_units": [
    {"singular": "second", "plural": "seconds"},
    {"singular": "minute", "plural": "minutes"},
    {"singular": "hour", "plural": "hours"}
  ]
}
