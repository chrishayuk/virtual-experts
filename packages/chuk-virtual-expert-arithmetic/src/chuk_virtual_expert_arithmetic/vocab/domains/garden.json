{
  "name": "garden",
  "description": "Gardening and plants",
  "agent_types": ["plant", "person"],
  "agent_templates": {
    "plant": {"pattern": "Plant ${letter}", "letters": ["A", "B"]},
    "tree": {"pattern": "Tree ${number}", "numbers": [1, 2]},
    "person": {"source": "names.people", "pattern": "${name}"}
  },
  "plants": ["tomato vine", "pepper plant", "rose bush", "sunflower", "apple tree", "orange tree"],
  "items": [
    {"singular": "tomato", "plural": "tomatoes"},
    {"singular": "flower", "plural": "flowers"},
    {"singular": "vegetable", "plural": "vegetables"},
    {"singular": "fruit", "plural": "fruits"},
    {"singular": "pepper", "plural": "peppers"}
  ],
  "verbs": {"singular": "produces", "plural": "produce"},
  "time_units": [
    {"singular": "day", "plural": "days"},
    {"singular": "week", "plural": "weeks"}
  ]
}
