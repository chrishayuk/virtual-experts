{
  "name": "school",
  "description": "Education and classroom activities",
  "agent_types": ["person", "classroom"],
  "agent_templates": {
    "person": {"source": "names.people", "pattern": "${name}"},
    "classroom": {"pattern": "Class ${letter}", "letters": ["A", "B"]}
  },
  "items": [
    {"singular": "student", "plural": "students"},
    {"singular": "book", "plural": "books"},
    {"singular": "pencil", "plural": "pencils"},
    {"singular": "worksheet", "plural": "worksheets"},
    {"singular": "problem", "plural": "problems"},
    {"singular": "question", "plural": "questions"}
  ],
  "verbs": {"singular": "completes", "plural": "complete"},
  "time_units": [
    {"singular": "hour", "plural": "hours"},
    {"singular": "day", "plural": "days"},
    {"singular": "week", "plural": "weeks"}
  ]
}
