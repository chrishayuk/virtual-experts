{
  "name": "travel",
  "description": "Movement and transportation",
  "agent_types": ["vehicle", "person"],
  "agent_templates": {
    "vehicle": {"pattern": "Car ${letter}", "letters": ["A", "B"]},
    "train": {"pattern": "Train ${number}", "numbers": [1, 2]},
    "bike": {"pattern": "Bike ${number}", "numbers": [1, 2]},
    "person": {"source": "names.people", "pattern": "${name}"}
  },
  "vehicles": ["car", "bus", "train", "bike", "truck", "van"],
  "items": [
    {"singular": "mile", "plural": "miles"},
    {"singular": "kilometer", "plural": "kilometers"},
    {"singular": "lap", "plural": "laps"},
    {"singular": "meter", "plural": "meters"},
    {"singular": "block", "plural": "blocks"}
  ],
  "verbs": {"singular": "travels", "plural": "travel"},
  "time_units": [
    {"singular": "hour", "plural": "hours"},
    {"singular": "minute", "plural": "minutes"}
  ]
}
